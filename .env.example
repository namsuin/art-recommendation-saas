# Supabase Configuration (Required for Database)
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# AI Services (Optional - at least one recommended)
# Google Cloud Vision API
GOOGLE_CLOUD_PROJECT_ID=your_google_cloud_project_id
GOOGLE_CLOUD_KEY_FILE=path_to_your_google_cloud_key_file.json

# Clarifai API
CLARIFAI_API_KEY=your_clarifai_api_key_here

# Replicate API (for advanced AI features)
REPLICATE_API_TOKEN=your_replicate_api_token_here

# Payment Processing (Optional)
STRIPE_SECRET_KEY=your_stripe_secret_key_here

# Server Configuration
PORT=3000
NODE_ENV=development
SESSION_TIMEOUT=1800

# Admin Configuration
# CRITICAL: Change this to a secure random token in production
ADMIN_AUTH_CODE=your-secure-admin-token-here